{
    "extends": "../tslint.json",
    "rules": {
        "directive-selector": [
            false,
            "attribute",
            "app",
            "camelCase"
        ],
        "component-selector": [
            false,
            "element",
            "app",
            "kebab-case"
        ],
      "no-inferrable-types": [
        true,
        "ignore-params",
        "ignore-properties"
      ]
    }
}
